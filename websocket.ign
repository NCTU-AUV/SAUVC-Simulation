<?xml version='1.0'?>
<ignition version='1.0'>
  <plugin name='ignition::launch::WebsocketServer'
          filename='ignition-launch-websocket-server'>
    <port>9002</port>
    <ssl>
      <cert_file>/ign-certs/localhost+2.pem</cert_file>
      <private_key_file>/ign-certs/localhost+2-key.pem</private_key_file>
    </ssl>
  </plugin>
</ignition>
